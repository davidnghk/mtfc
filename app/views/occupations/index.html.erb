<h1>Occupations</h1>

<table class="responsive nowrap table table-hover" id="dttb-occupations" data-sort="true" data-source="<%= url_for(format: :json)%>">
  <thead>
    <tr>
      <th data-data="location">Location</th>
      <th data-data="checkin_datetime">Checkin datetime</th>
      <th data-data="due_date">Due date</th>
      <th data-data="checkout_datetime">Checkout datetime</th>
      <th data-data="customer">Customer</th>
      <th data-data="passport">Passport</th>
      <th data-data="price">Price</th>
      <th data-data="total">Total</th>
      <th data-data="status">Status</th>
      <th data-data="url" data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export" width="100px"></th>
    </tr>
  </thead>

</table>

<br>

<%= link_to 'New Occupation', new_occupation_path %>