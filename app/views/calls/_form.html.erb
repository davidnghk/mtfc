
<%= simple_form_for(@call) do |f| %>

  <div class="form-inputs">
    <%= f.input :address %>
    <%= f.input :latitude %>
    <%= f.input :longitude %>
    <%= f.input :altitude %>
  </div>

    <div class="form-actions">
      <%= button_tag type: 'submit', class: "button my-button", id: "register-button" do %>
          <i class="fa fa-save fa-2x" aria-hidden="true"></i>
      <% end %>
    </div>
<% end %>
